import java.util.Scanner;

public class Armstrong {

	public static void main (String[] args) {
		int c = 0, a, temp;  
		Scanner sc = new Scanner(System.in);
	    int num=sc.nextInt();
	    temp = num;  
	    while(num > 0)  
	    {  
		    a = num % 10;  
		    num = num / 10;  
		    c = c + (a * a * a);  
	    }  
	    if(temp == c)  {
	    		System.out.println("Armstrong number");
	    }
	    else {  
	        System.out.println("Not Armstrong number");   
	   }
	}
}
